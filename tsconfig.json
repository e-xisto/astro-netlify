{
	"include": ["src"],
	"compilerOptions": {
	  "allowJs": true,
	  "module": "ES2020",
	  "outDir": "./dist",
	  "target": "ES2020",
	  "typeRoots": ["node_modules/@types", "node_modules/@netlify", "node"],
	  "declaration": true,
	  "emitDeclarationOnly": true,
	  "strict": true,
	  "moduleResolution": "node",
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	}
  }